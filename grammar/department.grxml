	<one-of>
		<item>校长办公室</item> 
		<item>校长室</item>
		
		<item>办公室</item>
		<item>秘书室</item>
		<item>车队</item>
		<item>文印</item>

		<item>组织人事处</item> 
		<item>人事处</item>
		<item>组织处</item> 

		<item>党委</item>
		<item>机关党委</item>

		<item>教务处</item>
		 
		<item>科研处</item> 

		<item>行政处</item> 
		<item>保卫科</item>
		<item>管理服务科</item>
		<item>膳食科</item>

		<item>财务处</item>

		<item>学员管理处</item> 

		<item>分校协调处</item> 

		<item>收发室</item>

		<item>老干处</item>
		<item>离退休干部处</item>
		<item>离退休干部管理处</item>

		<item>哲学</item>
		<item>哲学教研部</item>

		<item>经济学</item>
		<item>经济学教研部</item>

		<item>政治学</item>
		<item>政治学教研部</item>

		<item>法学</item>
		<item>法学教研部</item>

		<item>党史党建</item>
		<item>党史党建教研部</item>
		<item>党建教研部</item>

		<item>管理学</item>
		<item>管理学教研部</item>

		<item>行政学</item>
		<item>行政学教研部</item>

		<item>语文</item>
		<item>语文教研部</item>

		<item>马列所</item>
