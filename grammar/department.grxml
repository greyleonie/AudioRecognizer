<?xml version='1.0' encoding='GB'?>
<grammar xml:lang="zh-cn" version="1.0" xmlns="http://www.w3.org/2001/06/grammar" root="TroubleReport">

<meta name="incompletetimeout" content="1500"/>


<rule id="TroubleReport">

	<item repeat="0-1"> 
		<ruleref uri="#Department"/> 
	</item> 

	<item repeat="0-4"> 
		<ruleref uri="#DIGIT"/> 
	</item> 

</rule>

