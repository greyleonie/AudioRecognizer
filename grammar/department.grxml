		<item>信息中心</item>
		<item>信息网络中心</item>
		<item>网络中心</item>
		<item>电教</item>
		<item>电教中心</item>

		<item>报告厅</item>
		<item>一课室</item>
		<item>二课室</item>
		<item>三课室</item>
		<item>四课室</item>
		<item>五课室</item>
		<item>六课室</item>
		<item>七课室</item>
		<item>八课室</item>
		<item>礼堂</item>
		<item>阶梯教室</item>
	</one-of>
</rule>

<rule id="DIGIT"> 
  <one-of> 
   <item>0</item> 
   <item>1</item> 
   <item>2</item> 
   <item>3</item> 
   <item>4</item> 
   <item>5</item> 
   <item>6</item> 
   <item>7</item> 
   <item>8</item> 
   <item>9</item> 
  </one-of> 
</rule> 


</grammar>	